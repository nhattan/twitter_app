<% if @tweets.present? %>
  $('#tweets').prepend("<%= escape_javascript(render(@tweets, hidden: true)) %>")
  $('#since_id').val("<%= @tweets.first.id %>")
  tweets_length = $('.tweet.hidden').length
  $('#notification').html(tweets_length + " new tweets")
  $('#notification').parent().removeClass('hidden')
  document.title = "(" + tweets_length + ") Twitter"
<% end %>
